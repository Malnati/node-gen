{
  "name": "@codegenerator/node-gen",
  "version": "3.0.4",
  "description": "NodeJS code generator from database.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Malnati/node-gen.git"
  },
  "main": "dist/main.js",
  "bin": {
    "node-gen": "./dist/main.js",
    "db-reader-postgres": "dist/db.reader.postgres.js",
    "typeorm-entity-generator": "dist/typeorm-entity-generator.js",
    "service-generator": "dist/service-generator.js",
    "interface-generator": "dist/interface-generator.js",
    "controller-generator": "dist/controller-generator.js",
    "dto-generator": "dist/dto-generator.js",
    "module-generator": "dist/module-generator.js",
    "app-module-generator": "dist/app-module-generator.js",
    "main-generator": "dist/main-generator.js",
    "env-generator": "dist/env-generator.js",
    "package-json-generator": "dist/package-json-generator.js",
    "readme-generator": "dist/readme-generator.js"
  },
  "scripts": {
    "build": "tsc",
    "gen:uninstall": "npm uninstall -g @codegenerator/node-gen && npm list -g --depth=0",
    "gen:install": "npm install -g @codegenerator/node-gen@latest",
    "start": "npm run build && node dist/main.js",
    "first:publishing": "npm run build && npm org set codegenerator username owner && npm publish --access public",
    "publish:patch": "npm run build && git add . && git commit -m \"publishing a patch\" && npm version patch && npm publish",
    "publish:minor": "npm run build && git add . && git commit -m \"publishing a minor\" && npm version minor && npm publish",
    "publish:major": "npm run build && git add . && git commit -m \"publishing a major\" && npm version major && npm publish"
  },
  "keywords": [
    "code-generator",
    "NestJS",
    "TypeScript",
    "PostgreSQL",
    "typeorm",
    "database",
    "schema",
    "entities",
    "services",
    "controllers",
    "modules",
    "DTO",
    "automation",
    "project-setup",
    "backend",
    "nodejs",
    "typescript-generator",
    "nest-generator"
  ],
  "author": "Ricardo Malnati",
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/pg": "^8.11.6",
    "@types/yargs": "^17.0.32",
    "commander": "^12.1.0",
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "dotenv": "^16.4.5",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.0.2"
  }
}
